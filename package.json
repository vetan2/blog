{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "syncpack:list": "syncpack list-mismatches"
  },
  "devDependencies": {
    "@vetan2/eslint-config-base": "^0.1.4",
    "@vetan2/eslint-config-react": "^0.1.4",
    "@vetan2/eslint-config-typescript": "^0.1.4",
    "@vetan2/prettier-config": "^0.1.4",
    "eslint": "^8.36.0",
    "prettier": "^2.8.4",
    "syncpack": "^9.8.4",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "packageManager": "yarn@3.5.0"
}
